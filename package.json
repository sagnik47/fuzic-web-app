{
  "name": "fuzic-app",
  "version": "1.0.0",
  "description": "A web app to manage Spotify playlists - convert liked songs, merge playlists, and remove artist songs",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "build": "npm run build:css",
    "build:css": "tailwindcss -i ./src/styles/input.css -o ./public/styles/output.css --watch"
  },
  "keywords": ["spotify", "playlist", "music", "web-app"],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "spotify-web-api-node": "^5.0.2",
    "dotenv": "^16.0.3",
    "cors": "^2.8.5",
    "cookie-parser": "^1.4.6"
  },
  "devDependencies": {
    "nodemon": "^2.0.20",
    "tailwindcss": "^3.3.2"
  }
}
